digraph graph_g {
	graph [center=true rankdir=LR]
	node [fillcolor=gray90 fontname="Times-Roman" fontsize="12pt" shape=circle]
	edge [arrowsize=0.6 fontname="Times-Roman" fontsize="12pt" penwidth=0.6]
	qe
	qd
	qe -> qd [label="a: a\nt: d\n\n"]
	qe -> qe [label="d: d\n\n"]
	qd -> qd [label="a: a\nt: t\n\n"]
	qd -> qe [label="d: d\n\n"]
}
